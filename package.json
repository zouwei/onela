{
  "name": "onela",
  "version": "3.0.0",
  "description": "Based on the Node.js open-source ORM framework, it aims to enable seamless integration of a single set of business code with different types of databases.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zouwei/onela.git"
  },
  "keywords": [
    "onela",
    "orm",
    "orm framework",
    "Mysql",
    "PostgreSQL",
    "Postgre",
    "SQLite",
    "SQL Server",
    "object relational mapper"
  ],
  "author": "SHIYE",
  "license": "GPL-3.0-only",
  "maintainers": [
    {
      "name": "SHIYE",
      "email": "huzougege@gmail.com"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.9.2",
    "copyfiles": "^2.4.1",
    "rollup": "^4.52.5",
    "terser": "^5.44.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
